#VRML V2.0 utf8

EXTERNPROTO Raum [
] "basis/raum.wrl#Raum"

EXTERNPROTO Tisch [
    field SFVec3f scale
    field SFVec3f position
] "einrichtung/tisch.wrl#Tisch"

PROTO Zimmer [
    field SFVec3f scale 1 1 1
    field SFVec3f position 0 0 0
] {
    Transform {
        translation IS position
        scale IS scale
        children [
            Raum { }
            Tisch {
                position 9.25 0.625 3.75
            }
        ]
    }
}